  <body>
    <tu>
      <tuv xml:lang="sme">
        <seg>Skuvlen- ja oahppamateriáladoaimmahat</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Škovlim- já oppâmateriaaltoimâttâh</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Doaimmahaga fásta bargit</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Toimâttuv fastâpargeeh:</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Skuvlenčálli</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Škovlimčällee</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>- doaimmahaga hoavda</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>- toimâttuv hovdâ,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>- oahpahusa láhkaásaheami, hálddahusa, ruhtadanovdáneami, árvvoštallama, oahppaplánanorpmaid ja sámeoahpahusa ovdáneami čuovvun</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>- škovlim lahâasâttâs, haldâšem, ruttâdemovdánem, árvuštâllâm, máttááttâsvuávámnoormâi já sämimáttááttâs ovdánem čuávvum,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>- skuvlen- ja oahppamateriálalávdegotti čálli ja áššemeannudeaddji (skuvlenpolitihkalaš áššit ja almmolaš skuvlenáššit)</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>- škovlim- já oppâmateriaallävdikode čällee já oovdânpyehtee (škovlimpoolitlâš já almos škovlimääših),</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>- sámiid skuvlenáššiid ovttasbargoorgána čálli ja áššemeannudeaddji</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>- säämi škovlimašij otsâšpargo-orgaan čällee já oovdânpyehtee,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>- oktavuođadoallan sámeoahpahusa lágideaddji gielddaide, stáhta oahpahushálddahussii ja davviriikkalaš guoibmeorgánaidda</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>- ohtâvuođâtoollâm sämimáttááttâs uárnejeijee kieldáid, staatâ máttááttâshaaldâšmân já tave-eennâmlâš uábbiorgaanáid,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>- riikkaidgaskasaš ovdáneami čuovvun</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>- aalmugijkoskâsâš ovdánem čuávvum.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Oahppamateriálačálli</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Oppâmateriaalčällee</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>- skuvlen- ja oahppamateriálalávdegotti čálli ja áššemeannudeaddji (oahppamateriálaáššit)</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>- škovlim- já oppâmateriaallävdikode čällee já oovdânpyehtee (oppâmateriaalääših),</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>- oahppamateriálabarggu plánen, rávven, gohcin ja doaimmaheapmi</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>- oppâmateriaalpargo vuávám, stivrim, kocceem já toimâttempargo,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>- oahppamateriála válmmašteapmái gullevaš oktavuođaadoallan skuvllaide ja oahpahusa hálddahussii</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>- oppâmateriaal rähtimân lohtâseijee ohtâvuođâtoollâm škovláid já máttááttâs haaldâšmân,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>- davviriikkalaš ovttasbargu</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>- tave-eennâmlâš ohtsâšpargo.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Oahppamateriálaplánejeaddji</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Oppâmateriaalvuávájeijee</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>- oahppamateriála plánen-, doaimmahan- ja dieđihanbargu</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>- oppâmateriaal vuávám-, toimâttem- já tieđettempargo,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>- sámenuoraid dáiddadáhpáhusa válmmaštallan, diehtojuohkin, raporteren ja ruđaidgeavaheapmi</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>- Säminuorâi taaidâtábáhtus valmâštâllâm, tieđettem, raportistem já varijkevttim,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>- oahppamateriála válmmašteapmái gullevaš oktavuođadoallan skuvllaide ja davviriikkalaš ovttasbargu</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>- oppâmateriaal rähtimân lohtâseijee ohtâvuođâtoollâm škovláid já tave-eennâmlâš ohtsâšpargo,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Doaimmahatčálli</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Toimâttâhčällee</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>- arkiivva ja girjerádjosa bajásdoallan,</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>- arkkâduv já kirjerááju hoittám,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>- oahppamateriálaruđaid geavaheami čuovvun,</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>- oppâmateriaalvarij kevttim čuávvum,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>- oahppamateriálaid birra dieđiheapmi, vurken ja juohkin,</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>- oppâmateriaal tieđettempargo, vuorkkim já jyehim,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>- čoahkkimiid geavatlaš ordnen</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>- čuákkim vuáválâš orniimeh.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Oahppamateriála prošeaktabargit</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Merja Fofonoff, nuorttâsämikielâ</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Elen Ánne Sara, davvisámegiella</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Miina Seurujärvi, anarâškielâ</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Oona Länsman, davvisámegiella</seg>
      </tuv>
      <tuv xml:lang="smn">
        <seg>Neeta Jääskö, anarâškielâ</seg>
      </tuv>
    </tu>
