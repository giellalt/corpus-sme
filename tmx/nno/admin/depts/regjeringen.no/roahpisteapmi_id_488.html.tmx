  <body>
    <tu>
      <tuv xml:lang="sme">
        <seg>Departemeantta váldoulbmil lea bearráigeahččat ahte vuđolaš riektesihkarvuođadáhkádusat dollojuvvojit ja ovddiduvvojit.</seg>
      </tuv>
      <tuv xml:lang="nno">
        <seg>Departementet sine hovudmål er å sørge for at grunnleggjande rettstryggleiksgarantiar vert haldt ved like og utvikla.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Bajemus dási ulbmil lea sihkkarastit servodaga ja ovttaskas olbmuid oadjebasvuođa.</seg>
      </tuv>
      <tuv xml:lang="nno">
        <seg>Eit overordna mål er å sikre samfunnet og enkeltmennesket tryggleik.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Departemeantta váldobarggut::</seg>
      </tuv>
      <tuv xml:lang="nno">
        <seg>Departementet sine hovudoppgåver:</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Politihkalaš njunnožiid čállingoddi</seg>
      </tuv>
      <tuv xml:lang="nno">
        <seg>Sekretariat for politisk ledelse</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>ráhkkanahttit áššiid ráđđehussii, Gonagassii stáhtaráđis ja Stuoradiggái</seg>
      </tuv>
      <tuv xml:lang="nno">
        <seg>førebu saker for regjering, Kongen i statsråd og Stortinget</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>váikkuhit dasa ahte ráđđehusa politihkka čađahuvvo</seg>
      </tuv>
      <tuv xml:lang="nno">
        <seg>bidra til å gjennomføre regjeringa sin politikk</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Etáhtajođiheapmi</seg>
      </tuv>
      <tuv xml:lang="nno">
        <seg>Etatsleiing</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>ovddidit ja čuovvut olggobeal etáhtaid ja duopmostuoluid</seg>
      </tuv>
      <tuv xml:lang="nno">
        <seg>utvikling og oppfølgjing av ytre etatar og domstolane</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>• Hálddašeapmi</seg>
      </tuv>
      <tuv xml:lang="nno">
        <seg>Forvalting</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>hálddašanbargguid ovddasvástideaddji mat gusket ovttaskas olbmuide fitnodagaide, institušuvnnaide, suohkaniidda ja stáhta doaimmaide</seg>
      </tuv>
      <tuv xml:lang="nno">
        <seg>ansvar for forvaltingsoppgåver som berører enkeltpersonar, bedrifter, institusjonar, kommunar og statleg verksemd</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>bálvalusat álbmogii</seg>
      </tuv>
      <tuv xml:lang="nno">
        <seg>publikumsservice</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Justiisadepartemeantta váldonannensuorggit:</seg>
      </tuv>
      <tuv xml:lang="nno">
        <seg>Justisdepartementet sine hovudsatsingsområde:</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>geahpedit vearredaguid</seg>
      </tuv>
      <tuv xml:lang="nno">
        <seg>redusert kriminalitet</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>oadjebasvuohta ja riektesihkarvuohta</seg>
      </tuv>
      <tuv xml:lang="nno">
        <seg>tryggleik og rettssikkerheit</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>rabasvuohta ja demokratiija</seg>
      </tuv>
      <tuv xml:lang="nno">
        <seg>åpenheit og demokrati</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>• justiisadoaimmahaga bargguid bargat ja gozihit bálvalusaid álbmogii</seg>
      </tuv>
      <tuv xml:lang="nno">
        <seg>justisvesenet si oppgåveløysing og service til publikum</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Don gávnnat eanet dieđuid departemeantta bargosurggiid birra ossodagaid vuolde</seg>
      </tuv>
      <tuv xml:lang="nno">
        <seg>Meir om departementet sine arbeidsområde finn du under avdelingar.</seg>
      </tuv>
    </tu>
