    <tuv xml:lang="sme">
      <seg>Stáhtahálddahusossodat 
</seg>
    </tuv>
    <tuv xml:lang="nob">
      <seg>Statsforvaltningsavdelingen 
</seg>
    </tuv>
  </tu>
  <tu>
    <tuv xml:lang="sme">
      <seg>Ekspedišuvdnahoavda Kasper Holand , tlf. 22 24 42 85 Ovdakantuvra tlf. 22 24 49 99 
</seg>
    </tuv>
    <tuv xml:lang="nob">
      <seg>Ekspedisjonssjef Kasper Holand , tlf. 22 24 42 85 Forværelse tlf. 22 24 49 99 
</seg>
    </tuv>
  </tu>
  <tu>
    <tuv xml:lang="sme">
      <seg>Ossodagas leat guokte sekšuvnna ; Fylkkamánnesekšuvdna ja Etáhtastivrensekšuvdna . 
</seg>
    </tuv>
    <tuv xml:lang="nob">
      <seg>Avdelingen har to seksjoner ; Fylkesmannsseksjonen og Etatsstyringsseksjonen . 
</seg>
    </tuv>
  </tu>
  <tu>
    <tuv xml:lang="sme">
      <seg>Stáhtahálddahusossodagas leat čuovvovaš ovddasvástádussuorggit : 
</seg>
    </tuv>
    <tuv xml:lang="nob">
      <seg>Statsforvaltningsavdelingen har følgende ansvarsområder : 
</seg>
    </tuv>
  </tu>
  <tu>
    <tuv xml:lang="sme">
      <seg>Oppalaš bajimus ovddasvástádus regionála stáhtahálddahusa organiseremis ja juogus . 
</seg>
    </tuv>
    <tuv xml:lang="nob">
      <seg>Generelt overordnet ansvar for organisering og inndeling av regional statsforvaltning . 
</seg>
    </tuv>
  </tu>
  <tu>
    <tuv xml:lang="sme">
      <seg>Ovddasvástádus fylkkamánneámmáhiid jođiheamis ja ovddideamis . 
</seg>
    </tuv>
    <tuv xml:lang="nob">
      <seg>Ansvar for drift og utvikling av fylkesmannsembetene . 
</seg>
    </tuv>
  </tu>
  <tu>
    <tuv xml:lang="sme">
      <seg>Bajimus ovddasvástádus huksen- ja opmodatpolitihkas stáhtalaš siviila suorggis . 
</seg>
    </tuv>
    <tuv xml:lang="nob">
      <seg>Overordnet ansvar for bygge- og eiendomspolitikk i statlig sivil sektor . 
</seg>
    </tuv>
  </tu>
  <tu>
    <tuv xml:lang="sme">
      <seg>Statsbygga stivren . 
</seg>
    </tuv>
    <tuv xml:lang="nob">
      <seg>Styring av Statsbygg . 
</seg>
    </tuv>
  </tu>
  <tu>
    <tuv xml:lang="sme">
      <seg>Buot departemeanttaide gulli oktasašbálvalusat . 
</seg>
    </tuv>
    <tuv xml:lang="nob">
      <seg>Fellestjenester i departementsfellesskapet . 
</seg>
    </tuv>
  </tu>
  <tu>
    <tuv xml:lang="sme">
      <seg>Departemeanttaid bálvalusguovddáža ( DBGa ) jođiheapmi . 
</seg>
    </tuv>
    <tuv xml:lang="nob">
      <seg>Styring av Departementenes servicesenter ( DSS ) . 
</seg>
    </tuv>
  </tu>
  <tu>
    <tuv xml:lang="sme">
      <seg>Oasit peršuvdnasuodjaluspolitihkas , earret eará peršuvdnadiehtoláhkaásahus . 
</seg>
    </tuv>
    <tuv xml:lang="nob">
      <seg>Deler av personvernpolitikken , blant annet personopplysningsforskriften . 
</seg>
    </tuv>
  </tu>
  <tu>
    <tuv xml:lang="sme">
      <seg>Dáhtabearráigeahču stivren . 
</seg>
    </tuv>
    <tuv xml:lang="nob">
      <seg>Styring av Datatilsynet . 
</seg>
    </tuv>
  </tu>
  <tu>
    <tuv xml:lang="sme">
      <seg>Hálddahuslaš ovddasvástádus Peršuvdnasuodjalusnammagottis . 
</seg>
    </tuv>
    <tuv xml:lang="nob">
      <seg>
</seg>
    </tuv>
  </tu>
  <tu>
    <tuv xml:lang="sme">
      <seg>Gulahallandepartemeanta Gonagasa ja su sogalaččaid várás . 
</seg>
    </tuv>
    <tuv xml:lang="nob">
      <seg>Administrativt ansvar for Personvernnemnda . 
</seg>
    </tuv>
  </tu>
  <tu>
    <tuv xml:lang="sme">
      <seg>Bušeahttaovddasvástádus apanáššas ( Gonagasa ja su sogalaččaid jahkebálkkás ) ja Gonagashoavas . 
</seg>
    </tuv>
    <tuv xml:lang="nob">
