  <body>
    <tu>
      <tuv xml:lang="sme">
        <seg>Oahpa: Beaivi</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Lïerh: Biejjie</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg></seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Jaahkoe-byjhkesjimmie</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Mun oskkun Ipmil Áhččái,</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Manne Jupmele Aahtjan jaahkam,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>buotveagalažžii, almmi ja eatnama sivdnideaddjái.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>faamoehkommes, Elmien jïh eatnemen sjugniehtæjjese.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Mun oskkun Jesus Kristusii, Ipmila</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Manne Jeesus Kristusasse jaahkam,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>áidnoriegádan Bárdnái, min Hearrái,</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Jupmelen aajnege baarnan, mijjen åajvan</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>guhte sahkanii Bassi Vuoiŋŋas,</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>gie Aejlies Voejkenen baaktoe åedtjine sjidti,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>riegádii nieiddas Márjjás,</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Maarja niejteste reakasovvi,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg></seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Pontius Pilatusen baelien njåvtasovvi,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg></seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>kroessese noeresovvi, jeemi jïh kroeptese biejesovvi,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>russiinávlejuvvui, jámii ja</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>våålese jaemede-rijhkese vöölki,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>hávdáduvvui, njiejai jábmiid riikii,</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>gåalmeden biejjien jaemedistie tjuedtjeli,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg></seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>bæjjese Alman vöölki,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>bajásčuožžilii jábmiid luhtte</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Jupmelen, faamoehkommes Aehtjien,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg></seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>åelkies bielesne tjahkan,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>goalmmát beaivvi, manai bajás</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>gubpede vihth edtja bååstede båetedh,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>albmái, čohkká Ipmila, buotveagalaš</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>jielije jïh åemie almetjidie dööpmedh.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Áhči, olgeš gieđa bealde, boahtá fas</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Manne Aejlies Voejkenasse jaahkam,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>doppe dubmet elliid ja jábmiid.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>gærhkoen aejlies ektievoetese,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Mun oskkun Bassi Vuigŋii,</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>aejlies almetji tjåanghkenæmman,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>bassi oktasaš girku,</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>sådtoej luejhtiemasse,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>bassi olbmuid searvevuođa,</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>suttuid ándagassii addojumi, oačči</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>bajásčuožžileami ja agálaš eallima.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>åedtjien tjuedtjielæmman jïh ihkuven jieliedasse.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg></seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Aamen.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg></seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
