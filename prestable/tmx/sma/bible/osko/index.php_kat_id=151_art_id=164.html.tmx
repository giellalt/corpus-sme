<tmx>
  <header segtype="sentence" o-tmf="OmegaT TMX" adminlang="en-US" srclang="sme" datatype="plaintext">
    <prop type="x-filename">index.php_kat_id=151_art_id=164.html.xml</prop>
  </header>
  <body>
    <tu>
      <tuv xml:lang="sme">
        <seg></seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Lïerh: Låavth-gåetie</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Oahpa: Lávvu</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Aehtjie mijjen</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Áhččámet</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Áhččámet, don, guhte leat almmis !</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Aehtjie mijjen guhte leah Elmie-rijhkesne.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Basuhuvvos du namma.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Baajh dov nommem aejliestovvedh.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Bohtos du riika.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Šaddos du dáhttu,</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Baajh dov rijhkem båetedh.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>mo almmis nu maiddái eatnamis.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Baajh dov syjhtedem eatnamisnie</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg></seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>sjidtedh guktie Elmie-rijhkesne.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg></seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Vedtieh mijjese daan biejjien mijjen</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg></seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>fierhten-beajjetje laejpiem.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Atte midjiide min juohkebeaivválaš láibbi.. Ja atte midjiide min suttuid ándagassii,</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Luejhtieh mijjeste maam mijjieh meadteme guktie mijjieh luejhtebe dejstie guhth mijjese meadteme.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>nugo mii ge ándagassii addit velggolaččaidasamet.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Aellieh luejhtieh mijjem giehtjelimmij sijse,</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Ale doalvvo min geahččalussii,</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>muhto beastte min bahás eret.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>valla vaarjelh mijjem bahheste.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg></seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Juktie rijhke lea dov, faamoe jih earoe ihkuven aajkan.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg></seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Aamen.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Dasgo du lea riika, fápmu ja gudni</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>agálašvuhtii.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg></seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Jeesuse Aehtjie mijjen mijjem lieri.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Jesus oahpahii min rohkadallat Áhččámet, das leat čieža rohkosa, ja dát čieža rohkosa rohkodallet buot maid mii dárbbašat.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Dan lea tjïjhtje rohkelassh, jïh dah tjïjhtje rohkelassh voebnesjieh gaajhkem maam mijjieh daarpejibie rohkelassese.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg></seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
  </body>
