  <body>
    <tu>
      <tuv xml:lang="sme">
        <seg>Kristuskránsa</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Kristusen gievlie</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Eahpáraš gásttašuvvo</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Mus lea ránnjá gean namma lea Nils Ámmá ja mun ieš lean Bávval Niillas.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Moai leimme čuoigame ja guođoheame ealu beaivet.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Go veiggodii, de gullui eahpáraš bárggádeame moatte geardde munnuide.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Moai suorganeimme.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Ovtta iđiđa leimme fas vuolgime meahccái, de válddiime sálbmagirjji ja Áppesa mielde maid nahkeheimme ohci.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Das ledje golbma nuorra nieidda geat boagustedje munno go oidne moai válddime sálbmagirjji ja áppesa ohci.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Dat lohke munno šaddan ipmilbálolažžan.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Go de eahkeduvai, de álggii eahpáraš bárgut.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Moai čuorvvuime dan boahtit oidnosii suhkkes vuovddis.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>De bođii ja lei oaidnit dego liidnelurvu.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Moai jearaime eahpáraččas geat leigga su eadni ja áhčči.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Áhčči lei muhtin náitalan olmmoš, ja eadni lei nieida-olmmoš.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Moai siđaime eahpáračča mannat iežas vuoiŋŋadanbáikái.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Dat jávkkai dohko dan suhkkes vuovdái.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Mu ránnjá gásttašii su Juhanassan.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>De son válddii jeahkála eret geađggis ja bálkestii dohko dan guvlui gosa eahpáraš jávkkái ja vitkai dan.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Das maŋŋil dat ii šat gullon.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Sámás: Rávdná Turi Henriksen Gáldu: Qvigstad, J. (1927-1929): Samiske eventyr og sagn i boka: Samiske beretninger.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Pollan, Brita (red.) 2005: Utboren døpes.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>s. 370.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Aschehoug.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
