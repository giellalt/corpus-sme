  <body>
    <tu>
      <tuv xml:lang="sme">
        <seg>Kristuskránsa</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Kristusen gievlie</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Du soajáidat mu ala</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>1.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Du soajáidat mu ala, o Jesus lebbe don,</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>vai morraš sis ja ilus du luhte bisun mun.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Mu dorvu leage heađis, mu fápmu, suodjalus,</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>ja divtte juohke beaivvi, mu eallit árpmu sis.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>2.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Su suttuid ándagassii don atte, Beastážan,</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>ja sivdnit bassi miela ja dáhtu raddasán.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Min váldde, sihke stuorráid ja smávváid, háldosat,</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>ja divtte odne eahket min nohkkat ráfásat.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg></seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
  </body>
