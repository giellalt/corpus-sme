  <body>
    <tu>
      <tuv xml:lang="sme">
        <seg>Kristuskránsa</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Mii geiget guoros gieđaid</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Sámás (davvisámeg.):</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Rávdná Turi Henriksen, mai 2007</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Mii geiget guoros gieđaid</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>1 Mii geiget guoros gieđaid ja</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>du ovdii boahtit.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Min lusa boađe Ipmilan</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>du ádjagiin min eallimii.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>2 Du attáldagat ávkin leat</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>ja buorrin bohtet.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Min headjuvuođas gievrudat,</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>vai ovssiin urbbit luotkanit.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>3 Mii bajidit min gieđaid dál</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>ja rohkadallat:</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Don suddje sin geat gillájit</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>buot jikŋon váimmuid galbmasis.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>4 Min gieđaid guoros muora don</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>ieš sálle lieđđut,</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>vai šattuid guoddit eallimis</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>ja earáin háviid dálkkastit.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>5 Mii vuordit giđa maŋŋil fas</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>du árbmugeasi.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Kristusen gievlie</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Mas rievdá moraš, illu maid</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>ja duddjo šattuid čihkosis.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>6 Du árbmui oasálažžan leat</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>dat guoros gieđat.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Don addi Ipmil boađášit</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>min heajuid luhtte orošit !</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg></seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
  </body>
