  <body>
    <tu>
      <tuv xml:lang="sme">
        <seg>Oahpa: Girku</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Lïerh: Gærhkoe</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Sáme-girku Åankeres</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Åankere Åankere kapealla.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Saemien gærhkoe Åanghkeren njeeruvisnie</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Dán girku biras lea sámi biras.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Åanghkere-njeeruven gærhkoe.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Girkošilju birra leat máŋgga goađi.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Vååjnoe saemieh desnie årroeminie.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Ná lea leamaš guhkká.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Desnie jïjnjh saemien gåetieh.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Báikkis lea ” blåsjö-ássán ” - oassi smávvá barttaiguin, ja ” sámi ” - oassi gođiiguin ja buvrriiguin.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Numhtie guhkiem orreme; saemieh gåetide jïh buvride gærhkoen akten bealan tseegkin jïh laedtieh hæhtjojde mubpien bealan.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Dát guokte kultuvrra ellet bálddalagaid.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Dah göökte kultuvh desnie lægan.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Máŋga sápmelačča leat hávdáduvvon dán hávdeeatnamii.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Gaelmie-gaertienisnie jïjnjh saemieh juvlelgamme.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Árbevirolaš sámi girkostallamat leat buktán máŋgga muitinveara vásáhusaid.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Jïjnjh almetji leah buerie mojhtesh saemien gærhkoe-sjeltiej bïjre.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>dakkár vásáhusat čatnet buolvvaid oktii, ja danin nannejit oktasašvuođa.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Buerie mojhtesh båeries jïh noere almetjidie ektiedieh jïh mojhtesh jis ektievoetem nænnoestieh.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Mihcamáripmilbálvalusas leat sámi gávttehasat girkobalvat.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Jovnesåhkoen stoere gærhkoe-sjeltie Åanghkerisnie.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Ipmilbálvalusas geavahuvvo maiddái sámegiella dan láhkái ahte biibbalteavsttat, sálmmat ja liturgiijaoasit leat sámegillii.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Gosse gyrhkesjeminie, dellie gærhkoe-gaagkijh saemien vaarjoejgujmie. Saemien gïele gærhkosne govloe gosse Bijbelistie lohkeminie jïh saalmh laavloeminie.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Dát molsašuddá jagis jahkái.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Saemien liturgijem aaj utnieh.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg></seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Numhtie dejnie gïeline naan jaepiej jïh muvhtene jeatjhlaakan.</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg></seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
  </body>
