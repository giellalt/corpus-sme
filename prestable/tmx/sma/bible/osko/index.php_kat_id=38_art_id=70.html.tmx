  <body>
    <tu>
      <tuv xml:lang="sme">
        <seg>Kristuskránsa</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Áhččámet</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Áhččámet, don guhte leat almmis !</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Bauhuvvos du namma.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Bohtos du riika.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Šaddos du dáhttu,</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Kristusen gievlie</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>mo almmis nu maiddái eatnama alde.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Atte midjiide odne min beaivválaš láibbi.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Ja atte midjiide min suttuid ándagassii,</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>nugo mii ge ándagassi addit velggolaččaidasamet.</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Ale ge doalvvo min geahččalussii,</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>muhto beastte min báhas eret</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Dasgo du lea riika ja fápmu ja gudni agálašvuhtti</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg></seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
