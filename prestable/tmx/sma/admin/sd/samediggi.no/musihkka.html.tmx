      <tuv xml:lang="sma">
        <seg>Govlehtallijh:</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg></seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Ohtseme</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Emma Margret Skåden, tlf. 78 47 41 01 emma.margret.skaden@samediggi.no</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Doarjjaohcanskovvi</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Doarjjanjuolggadusat luođi ja sámi musihka várás</seg>
      </tuv>
      <tuv xml:lang="sma">
        <seg>Njoelkedassh ohtsemebaseradamme dåarjoe joejkese jïh saemien musihkese</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
