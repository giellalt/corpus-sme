  <body>
    <tu>
      <tuv xml:lang="sme">
        <seg>Kristuskránsa</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg>Kristuse-værjoe</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Go Jesus vádjolii eatnama alde, de lei son okta gii attii ráhkisvuođa.</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Son beroštii sis geaiguin deaivvadii ja hálidii sin veahkehit, ja dan son dagai danne og sus lei ráhkisvuohta olbmuide.</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Son jearai: Maid don dáhtut mu alccesat dahkat ?</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Ja son buoridii olbmuid.</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Dat lei maid okta vuohki addit ráhkisvuođa, okta vuohki juogadit ráhkisvuođa fámu.</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Son hálai olu ráhkisvuođa ja mieldeolmmošlašvuođa birra.</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Mii leat iežamet servodaga árvvuid bokte geahččalan buoremuddui dahkat duohtan su sániid das movt olbmot sáhttet eallit ovttas ráhkisvuođas.</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Su ustibat muosáhedje ustitvuođa maid eai goasse vajálduhttán.</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Loga dan maid son dajai ráhkisvuođa birra Johanasevangeliuma 3. kapihttalis, 34-35 vearssain</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg></seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
  </body>
