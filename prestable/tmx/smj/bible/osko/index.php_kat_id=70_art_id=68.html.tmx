  <body>
    <tu>
      <tuv xml:lang="sme">
        <seg>Kristuskránsa</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg>Kristuse-værjoe</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Biibbal muitala ahte Jesus lei Ipmila luhtte juo álggu rájes.</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Go Jesus šattai olmmožin, de guđii son iežas saji Ipmila guoras ja šattai olmmožin eatnama alde.</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Jesus gohčodii Ipmila áhččin.</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Son diđii ahte son gulai Ipmilii ja lei su mánná.</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Son dovddai Ipmila buorebut go oktage eará.</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Son muitalii ahte Ipmil lei sádden su olbmuid lusa.</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Loga álggu Johannesa vuosttaš kahpihttalis dakko gokko Jesus gohčoduvvo sátnin ja čuovgan.</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg></seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
  </body>
