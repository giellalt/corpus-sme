<tmx>
  <header segtype="sentence" o-tmf="OmegaT TMX" adminlang="en-US" srclang="sme" datatype="plaintext">
    <prop type="x-filename">index.php_kat_id=57_art_id=129.html.xml</prop>
  </header>
  <body>
    <tu>
      <tuv xml:lang="sme">
        <seg>Kristuskránsa</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg>Kristuse-værjoe</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Jesus vásihii sáttomeahci.</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Son lei doppe akto 40 beaivvi.</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Beargalat geahččalattai su; fáluhii sutnje fámu ja stuorisvuođa.</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Son válljjii eará geainnu.</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Vuollegašvuođa geainnu.</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Ráhkisvuođa geainnu.</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Son vásihii ” sáttomeahci ” maiddái eará háviid.</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Go eallin lei váttis, go ii oktage ipmirdan dan maid son dajai, ja go váivašuvai olbmuid behttosiid ja gáđašvuođa dihte.</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Sus ledje váttisvuođat, muhto ii son vuollánan, son doalahii oskkáldasat das maid diđii ahte lei riekta.</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Don sáhtát lohkat Lukasevangeliuma 4. kahpihttalis, vearssain 1-13 dan movt Jesus geahččaluvvui sáttomeahcis.</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg></seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
  </body>
</tmx>
