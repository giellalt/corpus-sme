<tmx>
  <header segtype="sentence" o-tmf="OmegaT TMX" adminlang="en-US" srclang="sme" datatype="plaintext">
    <prop type="x-filename">index.php_kat_id=56_art_id=128.html.xml</prop>
  </header>
  <body>
    <tu>
      <tuv xml:lang="sme">
        <seg>Kristuskránsa</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg>Kristuse-værjoe</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Maiddái Jesus gásttašuvvui.</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Lei gásttašeaddji Johanas gii su gásttašii.</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Go Jesus gásttašuvvui, de gullui jietna almmis mii celkkii: Don leat mu Bárdni gean mu ráhkistan.</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Dainna lágiin duođaštii Ipmil ahte Jesus lei su Bárdni, nugo Jesus lei cealkán.</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Ja olu olbmot gulle dan.</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Ipmila Vuoigŋa bođii su badjelii duvvá hámis.</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Jesus lei Ipmila mánná ja danne diđii son ahte Ipmil atná sus fuola.</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Son diđii ahte go rohkadalai, de sáhtii Ipmil su gullat ja sutnje vástidit.</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Jesusa gásttašeami birra sáhtát lohkat Lukasevangeliuma 3. kapihttalis, 21 ja 22 vearssain ja dasalassin paraleallaid</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg></seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg></seg>
      </tuv>
    </tu>
  </body>
</tmx>
