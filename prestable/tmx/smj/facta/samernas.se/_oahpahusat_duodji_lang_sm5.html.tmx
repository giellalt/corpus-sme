    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg></seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg>Dávverdiehto</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Ivdni &amp;amp; hápmi</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg>Bájnno ja Hábme</seg>
      </tuv>
    </tu>
    <tu>
      <tuv xml:lang="sme">
        <seg>Tema-vahkut čiekŋalit oahpuin veadde-, silba-, gámagoarrun-, herven ja bádjebargguin.</seg>
      </tuv>
      <tuv xml:lang="smj">
        <seg>Temávahko tjiegŋalup åhpajn dájvve-, silbba-, nuvtak-, tjátjek-, girjjam- ja smirjjimduojen.</seg>
      </tuv>
    </tu>
    <tu>
